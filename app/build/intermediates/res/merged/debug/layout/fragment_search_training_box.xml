<FrameLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/searchTraining_framelayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:padding="16dp"
        android:orientation="vertical">

        <ScrollView
            android:id="@+id/scrollLayout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#f1f9f5"
            android:layoutDirection="rtl"
            >

        <LinearLayout
            android:id="@+id/regitration_ground_content"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="90dp"

            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0.5">

                    <TextView
                        android:id="@+id/from_time_icon_fragment_register_fround"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center|right"
                        android:text="&#xf017;"
                        android:textColor="#20945a"
                        android:textSize="17sp" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1">

                    <TextView
                        android:id="@+id/from_time_prompt_fragment_register_fround"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center|right"
                        android:text="من"
                        android:textColor="#20945a"
                        android:textSize="18sp" />
                </LinearLayout>

                <Button
                    android:id="@+id/select_startTime_btn"
                    style="@style/button_reserv"
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:layout_marginBottom="5dp"
                    android:layout_marginTop="5dp"
                    android:layout_weight="1.25"
                    android:gravity="center"
                    android:text="اختار"
                    android:background="@drawable/rounded_with_effect"
                    android:textSize="18sp"

                    android:textAllCaps="false" />

                <TextView
                    android:id="@+id/to_time_icon_fragment_register_fround"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0.5"
                    android:gravity="center"
                    android:text="&#xf017;"
                    android:textColor="#20945a"
                    android:textSize="17sp" />

                <TextView
                    android:id="@+id/to_time_header_fragment_register_fround"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="الي "
                    android:textColor="#20945a"
                    android:textSize="17sp" />

                <Button
                    android:id="@+id/select_endTime_btn"
                    style="@style/button_reserv"
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:layout_marginBottom="5dp"
                    android:layout_marginTop="5dp"
                    android:layout_weight="1.25"
                    android:gravity="center"
                    android:text="اختار"
                    android:background="@drawable/rounded_with_effect"
                    android:textSize="18sp"

                    android:textAllCaps="false" />

            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp">

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0.5">

                    <TextView
                        android:id="@+id/ic_date"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="right|center"
                        android:text="&#xf073;"
                        android:textColor="#20945a"
                        android:textSize="17sp" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1">

                    <TextView
                        android:id="@+id/date_tv"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center|right"
                        android:text="التاريخ"
                        android:textColor="#20945a"
                        android:textSize="17sp" />
                </LinearLayout>

                <Button
                    android:id="@+id/select_date_button"
                    style="@style/button_reserv"
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:layout_marginBottom="5dp"
                    android:layout_marginTop="5dp"
                    android:layout_weight="4"
                    android:text="اختار"
                    android:background="@drawable/rounded_with_effect"
                    android:textSize="18sp"

                    android:textAllCaps="false"
                    android:textColor="#000" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp">

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0.5">

                    <TextView
                        android:id="@+id/ic_city"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="right|center"
                        android:text="&#xf041;"
                        android:textColor="#20945a"
                        android:textSize="17sp" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1">

                    <TextView
                        android:id="@+id/city_tv"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center|right"
                        android:text="المدينة"
                        android:textColor="#20945a"
                        android:textSize="17sp" />
                </LinearLayout>


                <Button
                    android:id="@+id/select_city_button"
                    style="@style/button_reserv"
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:layout_marginBottom="5dp"
                    android:layout_marginTop="5dp"
                    android:layout_weight="4"
                    android:background="@drawable/rounded_with_effect"
                    android:text="اختار"
                    android:textSize="18sp"

                    android:textAllCaps="false"
                    android:textColor="#000" />

            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp">

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0.5">

                    <TextView
                        android:id="@+id/playground_type_icon_fragment_search_training"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center|right"
                        android:text="&#xf1e3;"
                        android:textColor="#20945a"
                        android:textSize="17sp" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1">

                    <TextView
                        android:id="@+id/playground_type_header_fragment_search_training"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center|right"
                        android:text="الملعب"
                        android:textColor="#20945a"
                        android:textSize="17sp" />
                </LinearLayout>


                <Button
                    android:id="@+id/choose_playground_button"
                    style="@style/button_reserv"
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:layout_marginBottom="5dp"
                    android:layout_marginTop="5dp"
                    android:layout_weight="4"
                    android:background="@drawable/rounded_with_effect"
                    android:text="اختار"
                    android:textSize="18sp"

                    android:textAllCaps="false"
                    android:textColor="#000" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp">

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0.5">

                    <TextView
                        android:id="@+id/players_number_icon_fragment_search_training"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center|right"
                        android:text="&#xf0c0;"
                        android:textColor="#20945a"
                        android:textSize="17sp" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1">

                    <TextView
                        android:id="@+id/players_number_header_fragment_search_training"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center|right"
                        android:text="عدد اللاعبين"
                        android:textColor="#20945a"
                        android:textSize="17sp" />
                </LinearLayout>

                <Button
                    android:id="@+id/choose_Player_numbers_button"
                    style="@style/button_reserv"
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:layout_marginBottom="5dp"
                    android:layout_marginTop="5dp"
                    android:layout_weight="4"
                    android:background="@drawable/rounded_with_effect"
                    android:text="اختار"
                    android:textSize="18sp"

                    android:textAllCaps="false"
                    android:textColor="#000" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp">

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0.5">

                    <TextView
                        android:id="@+id/facilities_icon_fragment_search_training"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="right|center"
                        android:text="&#xf2cc;"
                        android:textColor="#20945a"
                        android:textSize="17sp" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1">

                    <TextView
                        android:id="@+id/facilities_header_fragment_search_training"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center|right"
                        android:text="الخدمات"
                        android:textColor="#20945a"
                        android:textSize="17sp" />

                </LinearLayout>

                <Button
                    android:id="@+id/choose_5admat_btn"
                    style="@style/button_reserv"
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:layout_marginBottom="5dp"
                    android:layout_marginTop="5dp"
                    android:layout_weight="4"
                    android:background="@drawable/rounded_with_effect"
                    android:text="اختار"
                    android:textAllCaps="false"
                    android:textSize="18sp"
                    android:textColor="#000" />

            </LinearLayout>


            <Button
                android:id="@+id/btn_fragment_search_training"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/search_trining_box_btn"
                android:onClick="viewResult"
                android:text="ابدا البحث"
                android:textAllCaps="false"
                android:layout_marginTop="8dp"
                android:gravity="center"
                android:textSize="24sp" />

        </LinearLayout>


</ScrollView>
    </FrameLayout>
