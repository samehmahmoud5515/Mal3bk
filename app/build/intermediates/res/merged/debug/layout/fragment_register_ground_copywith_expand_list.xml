<android.support.v4.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/drawer_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layoutDirection="rtl"

    >


    <!-- The main content view -->
    <LinearLayout
        android:layoutDirection="rtl"
        android:orientation="horizontal"
        android:id="@+id/content_frame"
        android:layout_width="match_parent"

        android:layout_height="match_parent" >


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="89.33dp"
                android:background="#ffffff"
                android:gravity="center_vertical|center_horizontal"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="26.67dp"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/toggleNav"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_weight="1"
                        android:src="@drawable/toggle1icon" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="81.33dp"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/first_prompt_regidter_ground"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"

                        android:fontFamily="fonts/DIN Next LT W23 Regular.ttf"
                        android:gravity="center"
                        android:text="حجز ملعب"
                        android:textColor="#008542"
                        android:textSize="21dp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="106.67dp"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/imageView17"
                        android:layout_width="17.33dp"
                        android:layout_height="17.33dp"
                        android:layout_weight="1"
                        android:background="@drawable/headericon" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/regitration_ground_content"
                android:layout_width="match_parent"
                android:layout_height="735dp"
                android:background="#f1f9f5"
                android:gravity="center"
                android:layoutDirection="rtl"
                android:orientation="vertical">

                <LinearLayout
                    android:id="@+id/map_reg_ground"
                    android:layout_width="400dp"
                    android:layout_height="170dp"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <com.google.android.gms.maps.MapView
                        android:id="@+id/mapView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1" />

                    <ImageView
                        android:id="@+id/imageView11"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                   />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="395dp"
                    android:layout_height="33.33dp"
                    android:layout_marginLeft="16dp"
                    android:layout_marginRight="16dp"
                    android:layout_marginTop="20dp"
                    android:layoutDirection="rtl"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:foregroundGravity="center_vertical"
                        android:gravity="center_vertical|center"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/imageView2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:background="@drawable/pickerdateiconoutdy" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="69.33dp"
                        android:layout_height="match_parent"
                        android:layout_marginRight="6.67dp"
                        android:foregroundGravity="center_vertical"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/second_prompt_regidter_ground"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:fontFamily="fonts/DIN Next LT W23 Regular.ttf"
                            android:text="تاريخ الحجز"
                            android:textColor="#20945a"
                            android:textSize="15dp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="285dp"
                        android:layout_height="match_parent"
                        android:layout_marginRight="16dp"
                        android:background="@drawable/layout_registerground_bg"
                        android:gravity="center"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="256dp"
                            android:layout_height="match_parent"
                            android:foregroundGravity="center_vertical"
                            android:gravity="center"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/calenderTXT"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text="TextView"
                                android:textAlignment="center" />

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:foregroundGravity="center_vertical"
                            android:gravity="center"
                            android:orientation="horizontal">

                            <ImageView
                                android:id="@+id/imageView9"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:background="@drawable/registrationdateiconin" />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="395dp"
                    android:layout_height="33.33dp"
                    android:layout_marginLeft="14.67dp"
                    android:layout_marginRight="14.67dp"
                    android:layout_marginTop="13.33dp"
                    android:foregroundGravity="center_vertical"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:foregroundGravity="center_vertical"
                        android:gravity="center_vertical|center"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/imageView7"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:background="@drawable/towniconout" />

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="69.33dp"
                        android:layout_height="match_parent"
                        android:layout_marginRight="6.67dp"
                        android:foregroundGravity="center_vertical"
                        android:gravity="center"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/third_prompt_regidter_ground"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:text="الموقع"
                            android:textColor="#20945a" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="285dp"
                        android:layout_height="match_parent"
                        android:layout_marginRight="16dp"
                        android:background="@drawable/layout_registerground_bg"
                        android:foregroundGravity="center_vertical"
                        android:gravity="center"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="256dp"
                            android:layout_height="match_parent"
                            android:foregroundGravity="center_vertical"
                            android:gravity="center"
                            android:orientation="horizontal">

                            <Spinner
                                android:id="@+id/townSpinner"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:background="#b2bab6"
                                android:popupBackground="#b2bab6" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:foregroundGravity="center_vertical"
                            android:gravity="center"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:background="@drawable/towniconin" />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="400dp"
                    android:layout_height="33.33dp"
                    android:layout_marginLeft="14.67dp"
                    android:layout_marginRight="14.67dp"
                    android:layout_marginTop="13.33dp"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="center_vertical|center"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/imageView5"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:background="@drawable/registrationdateiconout" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="69.33dp"
                        android:layout_height="match_parent"
                        android:layout_marginRight="6.67dp"
                        android:gravity="center"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/fourth_prompt_regidter_ground"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:text="وقت الحجز"
                            android:textColor="#20945a" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="285dp"
                        android:layout_height="match_parent"
                        android:layout_marginRight="16dp"
                        android:background="@drawable/layout_registerground_bg"
                        android:gravity="center"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="256dp"
                            android:layout_height="match_parent"
                            android:foregroundGravity="center_vertical"
                            android:gravity="center"
                            android:orientation="horizontal">

                            <Spinner
                                android:id="@+id/timeSpinner"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:background="#b2bab6"
                                android:popupBackground="#b2bab6" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:foregroundGravity="center_vertical"
                            android:gravity="center"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:background="@drawable/registrationdateicon" />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="266.67dp"
                    android:layout_height="57.33dp"
                    android:layout_marginBottom="32dp"
                    android:layout_marginLeft="14.67dp"
                    android:layout_marginRight="14.67dp"
                    android:layout_marginTop="37.33dp"
                    android:background="@drawable/layout_registerground_button"
                    android:orientation="horizontal">

                    <Button
                        android:id="@+id/fifth_prompt_regidter_ground"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="@drawable/layout_registerground_addcartbtn_bg"
                        android:onClick="viewResult"
                        android:paddingBottom="6dp"
                        android:text="ابدا البحث"
                        android:textSize="24dp" />
                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="109.33dp"
                android:gravity="center_vertical|center_horizontal"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="#ffffff"
                    android:gravity="center_vertical|center_horizontal"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:background="#ffffff"
                        android:gravity="center"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <ImageButton
                                android:id="@+id/imageButton"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"

                                android:onClick="registerGround"

                                />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="12dp"
                            android:gravity="center"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/textView4"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text="حجز ملعب"
                                android:textColor="#008542" />
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="94.67dp"
                        android:layout_height="match_parent"
                        android:layout_marginRight="24.67dp"
                        android:background="#ffffff"
                        android:gravity="center"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:orientation="horizontal">

                            <ImageButton
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="12dp"
                            android:gravity="center"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:autoText="true"
                                android:text="استقطاب لاعب"
                                android:textColor="#dddddd" />
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="93.33dp"
                        android:layout_height="match_parent"
                        android:layout_marginRight="33.33dp"
                        android:background="#ffffff"
                        android:gravity="center"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:orientation="horizontal">

                            <ImageButton
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"

                                android:foregroundGravity="center" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="12dp"
                            android:gravity="center"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text="البحث عن تمرين"
                                android:textColor="#dddddd" />
                        </LinearLayout>
                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>

        </LinearLayout>


    </LinearLayout>



    <!-- The navigation drawer -->

    <LinearLayout

        android:id="@+id/dockedLayout"
        android:layout_width="247dp"
        android:layout_height="wrap_content"
        android:layout_gravity="start"


        android:layout_marginTop="86.67dp"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#ffffff"

            android:gravity="center_horizontal"
            android:layoutDirection="rtl"
            android:orientation="vertical">

            <LinearLayout

                android:layout_width="153.33dp"
                android:layout_height="46.67dp"
                android:layout_marginTop="41.33dp"
                android:orientation="horizontal">

                <ImageView

                    android:id="@+id/nav_logo"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"

                    android:background="@drawable/mainlogoicon" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="202.67dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="49.33dp"

                android:background="#ffffff"
                android:gravity="center"
                android:orientation="vertical">



                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layoutDirection="rtl"
                    android:orientation="horizontal"
                    android:textDirection="rtl">


                    <ExpandableListView

                        android:id="@+id/laptop_list"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textDirection="rtl">test
                    </ExpandableListView>


                </LinearLayout>

                <View
                    android:id="@+id/view6"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="#dddddd" />


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="17.33dp"
                    android:layout_marginBottom="25.33dp"
                    android:layout_marginTop="25.33dp"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/nav_profile_icon"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"

                            android:background="@drawable/profile" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal">

                        <Button
                            android:id="@+id/nav_profile_btn"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_marginRight="4dp"
                            android:background="#ffffff"
                            android:gravity="right"
                            android:text="الحساب الشخصي"
                            android:textColor="#8a8a8a" />
                    </LinearLayout>

                </LinearLayout>

                <View
                    android:id="@+id/view2"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="#dddddd" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="17.33dp"
                    android:layout_marginBottom="25.33dp"
                    android:layout_marginTop="25.33dp"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/nav_team_icon"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"

                            android:background="@drawable/teamicon" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:orientation="horizontal">

                        <Button
                            android:id="@+id/nav_team_btn"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_marginRight="4dp"
                            android:layout_weight="1"
                            android:background="#ffffff"
                            android:gravity="right"
                            android:text="الفريق"
                            android:textColor="#8a8a8a" />
                    </LinearLayout>

                </LinearLayout>

                <View
                    android:id="@+id/view3"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="#dddddd" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="17.33dp"
                    android:layout_marginBottom="25.33dp"
                    android:layout_marginTop="25.33dp"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/nav_logout_icon"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"

                            android:background="@drawable/outicon" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal">

                        <Button
                            android:id="@+id/nav_logout_btn"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="4dp"
                            android:background="#ffffff"
                            android:gravity="right"
                            android:text="تسجيل خروج"
                            android:textColor="#8a8a8a" />

                    </LinearLayout>

                </LinearLayout>

                <View
                    android:id="@+id/view4"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="#dddddd" />

            </LinearLayout>

        </LinearLayout>
    </LinearLayout>

</android.support.v4.widget.DrawerLayout>


